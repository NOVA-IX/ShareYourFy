<div class="blogs__wraper bg-green-300  py-20 px-20">

	<div class="flex justify-between items-center">

		<% data.forEach(item => { %>
		<div class="blogs bg-white mr-5">
			<img src="https://images.unsplash.com/photo-1489396160836-2c99c977e970?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=800&q=60"
				class="">
			<div class="p-5">
				<h1 class="text-2xl font-bold text-green-800 py-2"><%= item.title%></h1>
				<p class="bg-white text-sm text-black"><%= item.instruments.join(", ")%></p>
				<a href="" class="py-2 px-3 mt-4 px-6 text-white bg-green-500 inline-block rounded"><%= item.phone%></a>
			</div>
		</div>
		<% }) %>
	</div>
</div>