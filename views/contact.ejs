                    <input type="email" style="border-width: 3px; border-color: #667EEA; width: 300px; height: 35px;"
                        name="email" id="email" placeholder="E-Mail ID">
                    <span style="padding-bottom: 10px;"></span>
                    <input type="password" style="border-width: 3px; border-color: #667EEA; width: 300px; height: 35px;"
                        name="pwd2" id="Pwd2" placeholder="Password">
