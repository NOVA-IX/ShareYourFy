<body>
    <%-include('partials/navbar')%>

    <div id="myModal" class="modal">
        <div class="modal-content" style="background-color: #1A202C;">
            <span class="close">&times;</span>
            <div class="form-container sign-up-container">
                <form action="#" id="signin">
                    <span style="padding-bottom: 10px; color: floralwhite; padding-top: 10px;">Sign-In</span>
                    <input type="text" style="border-width: 3px; border-color: #667EEA; width: 300px; height: 35px;"
                        name="un" id="Username" placeholder="Username">
                    <span style="padding-bottom: 10px;"></span>
                    <input type="password" style="border-width: 3px; border-color: #667EEA; width: 300px; height: 35px;"
                        name="pwd" id="Pwd" placeholder="Password">
                    <span style="padding-bottom: 20px;"></span>
                    <button type="submit" name="sub" id="SignInSubmit"
                        style="color: floralwhite; background-color: #667EEA; border-radius: 5px; width: 80px; height: 30px;">Login</button>
                    <h1 style="color: gray; padding-top: 20px; padding-bottom: 15px;">or<br></h1>
                    <button id="signupbtn"
                        class="inline-flex items-center bg-white-800 border-0 py-1 px-3 focus:outline-none hover:bg-gray-700 rounded text-base mt-4 md:mt-0"
                        style="color: floralwhite;">Sign Up
                        <svg fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round"
                            stroke-width="2" class="w-4 h-4 ml-1" viewBox="0 0 24 24">
                            <path d="M5 12h14M12 5l7 7-7 7"></path>
                        </svg>
                    </button>
                </form>
            </div>
        </div>
    </div>
